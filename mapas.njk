<!--SCRIPTS DE LA TABLA-->
<script type="">
									
	function mostrarTabla(){
		tablainh = document.getElementById("tabinith");
		boton2 = document.getElementById("mostrarmasTabla");
		var filastabla = document.querySelectorAll('#tabinit');

		var masfilastabla = document.querySelectorAll('#tabocult');
		
		if(tablainh.style.display == "none"){
			tablainh.style.display = "table-row";
			boton2.style.display = "block";
			for(var i=0; i<filastabla.length; i++){
				filastabla[i].style.display= "table-row";
			}
			document.getElementById("mostrarTabla").value = "Ocultar Tabla";
		}
		else{
			tablainh.style.display="none";
			boton2.style.display = "none";
			for(var i=0; i<filastabla.length; i++){
				filastabla[i].style.display= "none";
			}
			for(var i=0; i<masfilastabla.length; i++){
				masfilastabla[i].style.display= "none";
			}
			boton2.value="Ver más";
			document.getElementById("mostrarTabla").value = "Ver datos en tabla";
		}
		
	}

	function mostrarmasTabla(){
		var masfilastabla = document.querySelectorAll('#tabocult');
		
		if(masfilastabla[0].style.display == "none"){
			for(var i=0; i<masfilastabla.length; i++){
				masfilastabla[i].style.display= "table-row";
			}
			document.getElementById("mostrarmasTabla").value = "Ver menos";
		}
		else{
			for(var i=0; i<masfilastabla.length; i++){
				masfilastabla[i].style.display= "none";
			}
			document.getElementById("mostrarmasTabla").value = "Ver más";
		}

	}

</script>

