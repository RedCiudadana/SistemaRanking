{% extends "base.njk" %}

{% block head %}
<title> Mapas de datos Abiertos | Ranking Municipal</title>


<link rel="stylesheet" href="https://unpkg.com/leaflet@1.5.1/dist/leaflet.css" />
<script src="https://unpkg.com/leaflet@1.5.1/dist/leaflet.js"></script>

<script src="//ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<!-- CSS only -->
<link href="//cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
<!-- JavaScript Bundle with Popper -->
<script src="//cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>


<link rel="stylesheet" href="//cdn.datatables.net/1.11.3/css/jquery.dataTables.min.css"  />
<script src="//cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"></script>

<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">



{% endblock %}

{% block content %}
      <!-- breadcrumb-area-start -->
			<div class="breadcrumb-area pt-150 pb-155" style="background-image:url('/assets/SISTEMA RANKING/SLIDER/02.png');background-size:cover;">
				<div class="container">
					<div class="row">
						<div class="col-xl-6">
							<div class="breadcrumb-text text-center" style="padding: 0 2rem">
								<h2 class="text-white text-left" style="font-weight:1000;font-size:5rem">Ranking Municipal</h2>
								<h3 class="font-size-h4 font-size-h1-sm font-size-h4-lg font-size-h1-xl mb-0 text-white font-weight-bolder">
								</h3>
							</div>
						</div>
						<!--Paginas-->
						<div class="col-xl-6">

							
							
						</div>
					</div>
				</div>
			</div>
			<!-- breadcrumb-area-end -->

      <!--begin::Content-->
			<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
				<!--begin::Entry-->
				<div class="d-flex flex-column-fluid">
					<!--begin::Container-->
					<div class="container">
						<!--begin::Page Layout-->
						<div class="d-flex flex-row" style="display:flex; ">
							<!--begin::Layout-->
							<div class="flex-row-fluid ml-lg-8" style="margin:5% 20%!important; ">
								
								<div id="Mapa" class="w3-container tab border">
									<div id='map' style="height: 700px;"></div>
								</div>
								<div id="Datos" class="w3-container tab border" style="display:none;height: 100%;padding:5% 2%">
									<div>
										<br>											
										<table id="dtBasicExample" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
											<thead>
												<tr >
													<th class="text-center">Municipio</th>
													<th class="text-center">Departamento</th>
													<th class="text-center">Area Geográfica (Km<sup>2</sup>)</th>
												</tr>
											</thead>
											<tbody>
											{% for item in collections.muns %}
											<tr >
												<td class="tablecont text-center">{{item.data.muns.mun}}</td>
												<td class="tablecont text-center">{{item.data.muns.depto}}</td>
												<td class="tablecont text-center">{{item.data.muns.AREA_GEO}}</td>
											</tr>
											{% endfor %}
											</tbody>
										</table>
										
										<script src = "//cdn.datatables.net/1.10.18/js/jquery.dataTables.min.js" defer ></script>

										<script>
											$(document).ready(function () {
											$('#dtBasicExample').DataTable({
													"language":{
														"url":"//cdn.datatables.net/plug-ins/1.11.3/i18n/es_es.json"
														
													},
													"responsive": true
												});
											$('.dataTables_length').addClass('bs-select');
											});
										</script>
									</div>
								</div>
								<div id="Descargar" class="w3-container tab border align-self-center" style="display:none;padding:100px 25%;height: 700px;">
									<h2>Si quiere descargar los datos puede hacerlo del siguiente enlace </h2>
									<a class="w3-button" style="background:#c4c4c4" download="datos.json" href="/descargables/municipios.json">Descargar Datos</a>
								</div>
								<div id="Compartir" class="w3-container tab border" style="display:none;padding:100px 25%;height: 700px;"> 
									<h2>Copie el código para compartir la gráfica</h2>
									<code>
									&lt;embed src="https://sistemaranking.redciudadana.org/mapas/m1" type="text/html" class="w-100" &gt;
									</code>
								</div>
							

								<div style="display:flex;justify-content:space-around">
									<div class="col-lg-3 border" style="padding:0!important"><button class="w3-bar-item w3-button col-3 w-100" style="max-width:100%" onclick="openTab('Mapa')">Mapa</button></div>
									<div class="col-lg-3 border" style="padding:0!important"><button class="w3-bar-item w3-button col-3 w-100" style="max-width:100%" onclick="openTab('Datos')">Datos</button></div>
									<div class="col-lg-3 border" style="padding:0!important"><button class="w3-bar-item w3-button col-3 w-100" style="max-width:100%" onclick="openTab('Descargar')">Descargar</button></div>
									<div class="col-lg-3 border" style="padding:0!important"><button class="w3-bar-item w3-button col-3 w-100" style="max-width:100%" onclick="openTab('Compartir')">Compartir</button></div>	
								</div>


								
                            </div>
							<!--end::Layout-->
						</div>
						<!--end::Page Layout-->
					</div>
					<!--end::Container-->
				</div>
				<!--end::Entry-->
			</div>
			<!--end::Content-->
    </div>
		<!--end::Wrapper-->
	</div>
	<!--end::Page-->
</div>
<!--end::Main-->


